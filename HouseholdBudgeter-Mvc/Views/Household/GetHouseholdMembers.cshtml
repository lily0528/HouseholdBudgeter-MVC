@model List<UsersView>
@{
    ViewBag.Title = "GetHouseholdMembers";

}
<div class="card-header">
    <i class="fas fa-list-ul"></i>
    Household Members
</div>
<div class="card-body">
    <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
            <thead class="card-header">
                <tr>
                    <th>Id</th>
                    <th>Email</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var record in Model)
                {
                    <tr>
                        <td>@record.Id</td>
                        <td>@record.Email</td>
                    </tr>
                }
            </tbody>
        </table>
        <div>
            <strong>@Html.ActionLink("Back Last Page", "GetHouseholds")</strong>
        </div>
    </div>
</div>

